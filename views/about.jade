extends layout

block content
    div.page-wrapper
        nav.navbar.navbar-inverse.navbar-fixed-top
            .container
                // Brand and toggle get grouped for better mobile display
                .navbar-header
                    button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1', aria-expanded='false')
                        span.sr-only Toggle navigation
                        span.icon-bar
                        span.icon-bar
                        span.icon-bar
                    a.navbar-brand(href='#')
                        b Hello Nodej
                // Collect the nav links, forms, and other content for toggling
                #bs-example-navbar-collapse-1.collapse.navbar-collapse
                    ul.nav.navbar-nav
                        li.active
                            a(href='/')
                                | Home
                                span.sr-only (current)
                        li
                            a(href='/about') About
                        li
                            a(href='/contact') Contact
                        li.dropdown
                            a.dropdown-toggle(href='#', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false')
                                | Dropdown
                                span.caret
                            ul.dropdown-menu
                                li
                                    a(href='#') Action
                                li
                                    a(href='#') Another action
                                li
                                    a(href='#') Something else here
                                li.divider(role='separator')
                                li
                                    a(href='#') Separated link
                                li.divider(role='separator')
                                li
                                    a(href='#') One more separated link
                    form.navbar-form.navbar-left(role='search')
                        .form-group
                            input.form-control(type='text', placeholder='Search')
                    ul.nav.navbar-nav.navbar-right
                        li
                            button.btn.btn-default.navbar-btn(href='#', type='button') Sign In
                        li.dropdown
                            a.dropdown-toggle(href='#', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false')
                                | Dropdown
                                span.caret
                            ul.dropdown-menu
                                li
                                    a(href='#') Action
                                li
                                    a(href='#') Another action
                                li
                                    a(href='#') Something else here
                                li.divider(role='separator')
                                li
                                    a(href='#') Separated link

        .container
            .jumbotron
                h1 About Nodejs!
                p Fast, unopinionated, minimalist web framework for Node.js
                p
                    a.btn.btn-primary.btn-lg(href='#', role='button') Learn more
        footer(style="position:fixed;bottom:0;right:0;left:0")
            hr
            p.text-center   Copyright &copy HungLeExpress
        script(type='text/javascript').
            (function (html) {
                console.log(html);
            })(document.documentElement);